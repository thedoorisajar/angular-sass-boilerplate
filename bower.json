{
  "name": "angular-sass-boilerplate",
  "version": "0.0.0",
  "homepage": "https://github.com/thedoorisajar/angular-sass-boilerplate",
  "authors": [
    "thedoorisajar <thedoorisajar>"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.3.15",
    "angular-route": "~1.3.15",
    "angular-touch": "~1.3.15",
    "modernizr": "latest",
    "fontawesome": "latest",
    "normalize.css": "latest"
  },
  "install": {
    "path": {
      "css": "public/css/lib",
      "js": "public/js/lib",
      "scss": "public/sass",
      "eot": "public/fonts",
      "svg": "public/fonts",
      "ttf": "public/fonts",
      "otf": "public/fonts",
      "woff": "public/fonts",
      "woff2": "public/fonts"
    },
    "sources": {
      "modernizr": "bower_components/modernizr/modernizr.js",
      "fontawesome": [
        "bower_components/fontawesome/scss/*.scss",
        "bower_components/fontawesome/fonts/*.*"
      ]
    }
  },
  "devDependencies": {}
}
